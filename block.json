{
	"apiVersion": 2,
	"name": "geolonia/map",
	"version": "0.1.0",
	"title": "Geolonia Maps",
	"category": "widgets",
	"icon": "location",
	"description": "Add a simple map.",
	"supports": {
		"html": false
	},
	"textdomain": "geolonia-blocks",
	"attributes": {
		"lat": {
			"type": "number",
			"default": 0
		},
		"lng": {
			"type": "number",
			"default": 0
		},
		"zoom": {
			"type": "number",
			"default": 0
		},
		"style": {
			"type": "string",
			"default": "geolonia/basic"
		},
		"customStyle": {
			"type": "string",
			"default": ""
		},
		"pitch": {
			"type": "number",
			"default": 0
		},
		"bearing": {
			"type": "number",
			"default": 0
		},
		"description": {
			"type": "string",
			"default": ""
		}
	},
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
